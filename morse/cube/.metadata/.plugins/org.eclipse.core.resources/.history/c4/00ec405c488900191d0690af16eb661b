#ifndef MORS_H_
#define MORS_H_

void MainFunction(){
	
		  if (HAL_GPIO_ReadPin(Button_GPIO_Port, Button_Pin) == GPIO_PIN_SET) {
		  			HAL_Delay(100);
		  			if (HAL_GPIO_ReadPin(Button_GPIO_Port, Button_Pin) == GPIO_PIN_SET) {
		  				++MessageCounter;
		  				MessageLength = sprintf(DataToSend, "Wiadomosc nr %d\n\r", MessageCounter);
		  				CDC_Transmit_FS(DataToSend, MessageLength);
		  	}
		 }

}


#endif /* MORS_H_ */
